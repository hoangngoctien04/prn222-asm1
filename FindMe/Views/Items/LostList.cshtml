@model IEnumerable<FindMe.DAL.Models.Item>
@{
    ViewData["Title"] = "Lost Items";
}

<h2>Lost Items</h2>

<table class="table table-bordered">
    <tr>
        <th>Title</th>
        <th>Description</th>
        <th>Reported At</th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Title</td>
            <td>@item.Description</td>
            <td>@item.ReportedAt</td>
        </tr>
    }
</table>
